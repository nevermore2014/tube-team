<div class="video-card-list">
  <% @users.each do |user| %>
    <% if current_user == user %>
      <%= render partial: 'users/my_user_card.html.erb', locals: { user: user } %>
    <% else %>
      <%= render partial: 'users/user_card.html.erb', locals: { user: user } %>
    <% end %>
  <% end %>
</div>
